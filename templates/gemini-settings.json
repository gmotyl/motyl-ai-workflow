{
  "general": {
    "project_name": "{{PROJECT_NAME}}",
    "project_type": "{{PROJECT_TYPE}}",
    "team": "{{TEAM}}"
  },
  "model": {
    "provider": "google",
    "model_name": "gemini-pro",
    "temperature": 0.7,
    "max_tokens": 2048
  },
  "features": {
    "vision": true,
    "document_analysis": true,
    "code_understanding": true
  },
  "context": {
    "include_git_history": true,
    "max_context_size": 10000,
    "auto_summarize": true
  },
  "tools": {
    "bash": true,
    "file_system": true,
    "git": true
  },
  "session": {
    "auto_save": false,
    "session_directory": "progress/",
    "context_memory": true
  }
}
