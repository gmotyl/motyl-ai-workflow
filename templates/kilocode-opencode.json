{
  "provider": "kilo",
  "model": "kilocode-latest",
  "context": {
    "max_tokens": 8000,
    "include_git_history": true,
    "project_files_limit": 20
  },
  "permissions": {
    "file_system": "read-write",
    "bash_commands": true,
    "git_operations": true
  },
  "behavior": {
    "auto_save_sessions": true,
    "session_directory": "progress/",
    "temperature": 0.7,
    "autonomous_mode": false
  },
  "mcp_servers": []
}
